{
    "manifest_version": 2,
    "name": "MindMapData ",
    "version": "1.0",
    "description": "Stockage des mails",
    "permissions": [
      "messagesRead",
      "accountsRead",
      "accountsFolders"
      
    ],
    "background": {
      "scripts": ["background.js"]
    },
    "browser_action": {
      "default_popup": "popup/popup.html",
      "default_icon": {
        "16": "icon.png",
        "48": "icon.png",
        "128": "icon.png"
      }
    },
    "applications": {
      "gecko": {
        "id": "dd@ut3.com"
      }
    }
  }
  